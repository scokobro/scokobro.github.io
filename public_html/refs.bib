	Journal = {Television New Media},
	Keywords = {imported},
	Pages = {1527476409358087},
	Title = {The Banality of Boundaries: Performance of the Nation in a {J}apanese Television Comedy},
	Url = {http://tvn.sagepub.com/cgi/content/abstract/1527476409358087v1},
	Year = 2010,
