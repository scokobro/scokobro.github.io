	Journal = {Perspectives on Psychological Science},
	Keywords = {cinema, decision, film, rationality},
	Number = 2,
	Owner = {Administrator},
	Pages = {162--180},
	Timestamp = {2009.06.17},
	Title = {{``Leaky''} Rationality: How Research on Behavioral Decision Making Challenges Normative Standards of Rationality},
