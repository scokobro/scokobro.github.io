	Keywords = {Visuo-motor behavior},
	Number = {14},
	Pages = {4031 - 4037},
	Title = {Bisections in two languages: When number processing, spatial representation, and habitual reading direction interact},
	Url = {http://www.sciencedirect.com/science/article/B6T0D-518TF0J-3/2/421cceef90aa8320cc6eaa587150bb02},
