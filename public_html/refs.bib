	N1 = {DavidMachin10012006, /Users/home/Desktop/PhD/Archive video footage in news- creating a likeness and index of the phenomenal world.pdf article},
	Number = {3},
	Pages = {345-366},
	Title = {Archive Video Footage in News: Creating a Likeness and Index of the Phenomenal World},
